<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Order.
    -->
    <changeSet id="20220414103431-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="delivery_man_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order__delivery_man_id"
                                 referencedColumnNames="id"
                                 referencedTableName="delivery_man"/>

        <addForeignKeyConstraint baseColumnNames="trader_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order__trader_id"
                                 referencedColumnNames="id"
                                 referencedTableName="trader"/>
    </changeSet>
</databaseChangeLog>
